@{
    ViewData["Title"] = "Home Page";
}

<div id="myCarousel" class="carousel slide" data-ride="carousel" data-interval="6000">
    <ol class="carousel-indicators">
        <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
        <li data-target="#myCarousel" data-slide-to="1"></li>
        <li data-target="#myCarousel" data-slide-to="2"></li>
        <li data-target="#myCarousel" data-slide-to="3"></li>
    </ol>
    <div class="carousel-inner" role="listbox">
        <div class="item active">
            <img src="~/images/ship-image.jpg" alt="ASP.NET" class="img-responsive" style="width: 1140px; height: 375px"/>
            <div class="carousel-caption" role="option">
                <p>
                    CONNECT your business with the world
                    <a class="btn btn-default" href="http://fww-logistics.com/">
                        Learn More
                    </a>
                </p>
            </div>
        </div>
        <div class="item">
            <img src="~/images/container-817406-1024x683.jpg" alt="Visual Studio" class="img-responsive" style="width: 1140px; height: 375px"/>
            <div class="carousel-caption" role="option">
              <p>
                Air and Ocean Freight
                <a class="btn btn-default" href="http://fww-logistics.com/">
                  Learn More
                </a>
              </p>
            </div>
        </div>
        <div class="item">
            <img src="~/images/container-truck-1024x683.jpg" alt="Package Management" class="img-responsive" style="width: 1140px; height: 375px"/>
            <div class="carousel-caption" role="option">
              <p>
                Container Drayage
                <a class="btn btn-default" href="http://fww-logistics.com/">
                  Learn More
                </a>
              </p>
            </div>
        </div>
        <div class="item">
            <img src="~/images/car.jpg" alt="Microsoft Azure" class="img-responsive" style="width: 1140px; height: 375px"/>
            <div class="carousel-caption" role="option">
              <p>
                Vehicle Shipping
                <a class="btn btn-default" href="http://fww-logistics.com/">
                  Learn More
                </a>
              </p>
            </div>
        </div>
    </div>
    <a class="left carousel-control" href="#myCarousel" role="button" data-slide="prev">
        <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="right carousel-control" href="#myCarousel" role="button" data-slide="next">
        <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
    </a>
</div>

<div class="row">
  <div class="col-md-12">
    <h1>Services</h1>
    <p style="font-size: 16px; font-family: verdana; color: green">
      We proudly provide customized shipping & logistics solutions through air, ocean and land that suit your unique business needs. While maintaining a number of well-established connections with world-leading carriers, we make sure our customers receive only the most affordable prices, best quality of services and timely arrival and delivery of their goods.
    </p>
  </div>
</div>

<div class="row">
  <div class="col-md-3">
    <h3>Air and Ocean Freight</h3>
    <p class="pull-right" style="font-size: 14px; font-family: verdana; color: steelblue; width: 90%">
      Our one-stop air and ocean freight services are tailored just for you to suit all your unique business needs.
    </p>
  </div>
  <div class="col-md-3">
    <h3>Container Drayage</h3>
    <p class="pull-right" style="font-size: 14px; font-family: verdana; color: steelblue; width: 90%">
      We offer you on-time transportation of your marine containers at cost-competitive prices by outsourcing the service to dedicated container drayage fleets.
    </p>
  </div>
  <div class="col-md-3">
    <h3>Vehicle Shipping</h3>
    <p class="pull-right" style="font-size: 14px; font-family: verdana; color: steelblue; width: 90%">
      WE LOVE CARS. Just as much as you do. That's why we fully understand the importance of safe and flawless shipping of yours vehicles.
    </p>
  </div>
  <div class="col-md-3">
    <h3 style="margin-bottom: 2px">Personal Effects&nbsp;</h3>
    <h3 style="margin-top: 2px">International Relocation</h3>
    <p class="pull-right" style="font-size: 14px; font-family: verdana; color: steelblue; width: 90%">
      Home is wherever you are. We help you with moving and relocation to the place in your mind, including domestic long distance, cross-border, and international overseas.
    </p>
  </div>
</div>
